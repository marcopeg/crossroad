{
  "name": "@crossroad/rules",
  "description": "Regulates requests to Crossroad APIs",
  "version": "0.2.13",
  "author": "Marco Pegoraro <marco.pegoraro@gmail.com",
  "bugs": {
    "url": "https://github.com/marcopeg/crossroad/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@crossroad/manager": "^0.2.13",
    "@crossroad/schema": "^0.2.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@forrestjs/babel-preset-package": "^2.9.4",
    "cross-env": "^5.2.0"
  },
  "homepage": "https://github.com/marcopeg/crossroad/tree/master/lib/rules#readme",
  "keywords": [
    "graphql",
    "schema",
    "registry"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/marcopeg/crossroad/tree/master/lib/rules",
  "scripts": {
    "clean": "rm -rf node_modules && rm -f yarn.lock && rm -f package-lock.json",
    "test": "jest",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "watch:to": "babel -w src --out-dir \"${TARGET}/node_modules/${npm_package_name}\" --ignore **/__tests__"
  }
}
